###############
### DESCRIPCION
###############
# Contiene información de eventos de f1, como quien ha ganado, donde fue
# wdt:P31 = grand_prix -> representa cada grand prix de f1 que existe
# (no va por años, eso se explica en la siguiente linea)
# wdt:P31 = f1_results -> resultados de un evento, para un año especíifco, por ejemplo ganador de Spanish Grand Prix 2023
# guarda en la relacion t8:f1_event_type a que evento hace referencia (en el ejemplo anterior, seria t8:f1_event_type = q8:spanish_grand_prix)
###############
### EJEMPLOS DE CONSULTAS
###############
#

###############
### CONTENIDO
###############

# Spanish Grand Prix
q8:spanish_grand_prix t8:name "Spanish Grand Prix" ;
                      t8:held_at q8:circuit_de_catalunya ;
                      t8:is_part_of q8:formula_1_series ;
                      wdt:P31 q8:grand_prix .

q8:spanish_gp_2023_results t8:year "2023" ;
    t8:f1_event_type q8:spanish_grand_prix ;
    t8:winner q8:max_verstappen ;
    t8:runner_up q8:lewis_hamilton ;
    t8:third_place q8:fernando_alonso ;
    wdt:P31 q8:f1_results .

q8:spanish_gp_2022_results t8:year "2022" ;
    t8:f1_event_type q8:spanish_grand_prix ;
    t8:winner q8:max_verstappen ;
    t8:runner_up q8:charles_leclerc ;
    t8:third_place q8:george_russell ;
    wdt:P31 q8:f1_results .

q8:spanish_gp_2021_results t8:year "2021" ;
    t8:f1_event_type q8:spanish_grand_prix ;
    t8:winner q8:lewis_hamilton ;
    t8:runner_up q8:max_verstappen ;
    t8:third_place q8:valtteri_bottas ;
    wdt:P31 q8:f1_results .

q8:circuit_de_catalunya t8:is_circuit "Circuit de Barcelona-Catalunya" ;
t8:hosts q8:spanish_grand_prix ;
t8:length "4.675 km" .

